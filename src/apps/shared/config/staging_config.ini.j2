[application]
error_reporting=E_ALL
display_errors=1
display_startup_errors=1
env=staging
send_single_validations=1
db_master_slave_enabled=1

[database]
adapter=Mysql
host=stagingdatabases.cywoihxulkwc.eu-west-1.rds.amazonaws.com
username={{ database_user }}
password={{ database_pass }}
dbname=euromillions
original_db_name=euromillions
is_dev_mode=true

[database_slave]
adapter=Mysql
host=stagingdatabases-read-replica.cywoihxulkwc.eu-west-1.rds.amazonaws.com
username={{ database_slave_user }}
password={{ database_slave_pass }}
dbname=euromillions

[domain]
url=beta.euromillions.com/

[redis]
